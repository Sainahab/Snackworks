{
    "name": "genealabs/laravel-caffeine",
    "description": "Keeping Your Laravel Forms Awake",
    "license": "MIT",
    "authors": [
        {
            "name": "Mike Bronner",
            "email": "hello@genealabs.com"
        }
    ],
    "require": {
        "illuminate/routing": "^7.0",
        "illuminate/support": "^7.0",
        "jenssegers/model": "^1.3"
    },
    "require-dev": {
        "orchestra/testbench-browser-kit": "^5.0",
        "orchestra/testbench-dusk": "^5.0",
        "orchestra/testbench": "^5.0",
        "phpmd/phpmd": "^2.7",
        "phpunit/phpunit": "^8.0",
        "php-coveralls/php-coveralls" : "^2.2",
        "sebastian/phpcpd": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "GeneaLabs\\LaravelCaffeine\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GeneaLabs\\LaravelCaffeine\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "GeneaLabs\\LaravelCaffeine\\Providers\\Service"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
